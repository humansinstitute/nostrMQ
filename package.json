{
  "name": "nostrmq",
  "version": "0.3.0",
  "type": "module",
  "description": "A minimal Node.js message-queue library for encrypted RPC over Nostr",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": ["nostr", "mq", "rpc", "nip04", "nip13", "pow"],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "nostr-tools": "^2.0.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "typescript": "^5.4.0",
    "@types/ws": "^8.5.8"
  },
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/nostrmq.git"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
